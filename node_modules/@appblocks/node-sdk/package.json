    "npm": ">=8.0.0",
    "node": ">=15.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/eslint": "^8.21.3",
    "@types/jest": "^29.5.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "lint-staged": "^13.2.0"
